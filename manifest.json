{
    "manifest_version": 2,
    "name": "fake new detect",
    "version": "0.1",
    "description": "simple fake news detection on different plateforms",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["index.js"]
      }
    ],
    "icons": 
    {
      "16": "icons/16x16.jfif",
      "48": "icons/48x48.jfif",
      "128": "icons/128x128.jfif"
    },
    "web_accessible_resources": [
      "index.html"
    ]
}